[diff]
    external = git_diff_wrapper
[pager]
    diff =
[alias]
    st = status -sb
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log --pretty=format:'%Cred%h%Creset -%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
[user]
	email = hdd@zhihu.com
	name = hdd
[color]
	ui = true
[push]
	default = matching
